@{
    ViewData["Title"] = @Model.resultList[0]["event_name"];
}

<div class="container px-5 pt-5">
    <h2 class="display-2 mb-4 text-primary fw-bolder">@Model.resultList[0]["event_name"]</h2>
    <p class="mb-4 fs-3 fw-bold text-danger">Event starts at: @Model.resultList[0]["date"]</p>
    <p class="mb-4 fs-4 fst-italic text-primary-emphasis">@Model.resultList[0]["description"]</p>
    <p class="fs-3 text-primary-emphasis">@Model.resultList[0]["content_body"]</p>
</div>
